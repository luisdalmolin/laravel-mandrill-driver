{
    "name": "therobfonz/laravel-mandrill-driver",
    "description": "Mandrill Driver for Laravel",
    "type": "library",
    "keywords": [
        "Laravel",
        "Mandrill"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Dalmolin",
            "email": "luis.nh@gmail.com"
        },
        {
            "name": "Rob Fonseca",
            "email": "robfonseca@gmail.com"
        },
        {
            "name": "Brandon Ferens",
            "email": "brandon@kirschbaumdevelopment.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.0|^12.0",
        "mailchimp/transactional": "^1.0",
        "symfony/mailer": "^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^9.9|^10.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelMandrill\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelMandrill\\MandrillServiceProvider"
            ]
        }
    }
}
