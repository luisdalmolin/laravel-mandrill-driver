{
    "name": "therobfonz/laravel-mandrill-driver",
    "description": "Mandrill Driver for Laravel 7+",
    "type": "library",
    "keywords": [
        "Laravel",
        "Mandrill"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Fonseca",
            "email": "robfonseca@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "LaravelMandrill\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelMandrill\\MandrillServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
